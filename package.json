{
	"name": "guteblock",
	"version": "1.0.0",
	"description": "",
	"main": "index.js",
	"scripts": {
		"checkFormat": "prettier --check \"**/*.{js,json,css,scss}\"",
		"format": "prettier --write \"**/*.{js,json,css,scss}\"",
		"lint": "eslint src",
		"start": "webpack --mode=development --watch",
		"build": "npm run lint && webpack --mode=production && gulp bundle"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"src/**/*.js": "eslint",
		"**/*.{js,json,css,scss}": [
			"prettier --write",
			"git add"
		]
	},
	"author": "",
	"license": "ISC",
	"dependencies": {
		"@wordpress/blob": "^2.5.1",
		"@wordpress/blocks": "^6.7.2",
		"@wordpress/html-entities": "^2.5.0",
		"@wordpress/i18n": "^3.6.1",
		"react-countup": "^4.2.3",
		"react-sortable-hoc": "^1.10.1",
		"serialize-javascript": "^2.1.2"
	},
	"devDependencies": {
		"@babel/core": "^7.6.4",
		"@babel/plugin-proposal-class-properties": "^7.7.0",
		"@babel/preset-env": "^7.6.3",
		"@babel/preset-react": "^7.6.3",
		"@wordpress/browserslist-config": "^2.6.0",
		"@wordpress/components": "^8.3.2",
		"@wordpress/data": "^4.10.0",
		"@wordpress/editor": "^9.7.5",
		"@wordpress/element": "^2.8.2",
		"autoprefixer": "^9.7.1",
		"babel-eslint": "^10.0.3",
		"babel-loader": "^8.0.6",
		"browserslist": "^4.7.2",
		"classnames": "^2.2.6",
		"clean-webpack-plugin": "^3.0.0",
		"css-loader": "^3.2.0",
		"eslint": "^6.6.0",
		"eslint-config-prettier": "^6.5.0",
		"eslint-loader": "^3.0.2",
		"eslint-plugin-react": "^7.16.0",
		"file-loader": "^4.3.0",
		"gulp": "^4.0.2",
		"gulp-zip": "^5.0.1",
		"husky": "^3.0.9",
		"jquery": "^3.4.1",
		"lint-staged": "^9.4.2",
